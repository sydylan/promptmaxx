{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "promptmaxx",
  "identifier": "com.promptmaxx.app",
  "version": "0.1.0",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "promptmaxx",
        "width": 200,
        "height": 60,
        "minWidth": 200,
        "minHeight": 60,
        "resizable": false,
        "decorations": false,
        "transparent": true,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "visible": true,
        "focus": false,
        "x": 100,
        "y": 50
      }
    ],
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app", "dmg"],
    "icon": []
  },
  "plugins": {}
}
